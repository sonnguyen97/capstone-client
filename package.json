{
  "name": "email-marketing-management-for-shopify-platform",
  "version": "0.0.1",
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "dev": "set PORT=3001 && react-app-rewired start",
    "start": "npx serve -s build",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "eslint --ext .js,jsx src",
    "tslint": "tslint \"src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "rules": {
    "jsx-a11y/label-has-associated-control": [
      2,
      {
        "labelComponents": [
          "Upload"
        ],
        "labelAttributes": [
          "label"
        ],
        "depth": 2
      }
    ]
  },
  "dependencies": {
    "@rowno/sparkline": "^3.0.1",
    "antd": "^3.13.2",
    "axios": "^0.19.0",
    "bootstrap-css-only": "^4.2.1",
    "chart.js": "^2.7.3",
    "chartist": "^0.11.0",
    "chartist-plugin-tooltips-updated": "^0.0.20",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.1",
    "d3-dsv": "^1.1.1",
    "d3-format": "^1.3.2",
    "d3-time-format": "^2.1.3",
    "dotenv": "^8.2.0",
    "enquire-js": "^0.2.1",
    "firebase": "^5.8.2",
    "history": "^4.7.2",
    "html-to-text": "^5.1.1",
    "lodash": "^4.17.11",
    "nprogress": "^0.2.0",
    "peity-react": "^0.0.7",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "prop-types": "^15.7.1",
    "qs": "^6.6.0",
    "rc-drawer": "^1.7.7",
    "react": "^16.8.1",
    "react-c3js": "^0.1.20",
    "react-chartist": "^0.13.3",
    "react-chartjs-2": "^2.7.4",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.1",
    "react-draft-wysiwyg": "^1.13.1",
    "react-email-editor": "^1.0.0",
    "react-helmet": "^5.2.0",
    "react-highlight-words": "^0.16.0",
    "react-intl": "^2.8.0",
    "react-loadable": "^5.5.0",
    "react-moment": "^0.9.6",
    "react-redux": "^6.0.0",
    "react-redux-spinner": "^2.0.0",
    "react-render-html": "^0.6.0",
    "react-router": "^4.3",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.2.0",
    "react-simple-flowchart": "^1.2.2",
    "react-stockcharts": "^0.7.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "reqwest": "^2.0.5",
    "sequelize": "^5.19.4",
    "store": "^2.0.12",
    "styled-components": "^4.4.1",
    "validator": "^12.1.0",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.11",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.11.0",
    "react-app-rewired": "^2.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
